    "@uppy/xhr-upload": "^1.0.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.4.0",
    "date-fns": "^1.30.1",
    "fbemitter": "^2.1.1",
    "formdata-polyfill": "^3.0.17",
    "history": "^4.9.0",
    "hoist-non-react-statics": "^3.3.0",
    "immutability-helper": "^3.0.0",
    "laravel-echo": "^1.5.3",
    "lazysizes": "^4.1.8",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.4",
    "next-frame": "^0.2.3",
    "pusher-js": "^4.4.0",
    "rc-time-picker": "^3.6.4",
    "react": "^16.8.6",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-html-parser": "^2.0.2",
    "react-icons": "^2.2.7",
    "react-loadable": "^5.5.0",
    "react-notify-toast": "^0.5.0",
    "react-proptype-conditional-require": "^1.0.4",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-router-scroll-4": "^1.0.0-beta.2",
    "react-scripts": "^3.0.0",
    "react-select": "^2.4.3",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reflective-bind": "^0.2.3",
    "smooth-scroll-into-view-if-needed": "^1.1.23",
    "superagent": "^5.0.2",
    "throttle-debounce": "^2.1.0",
    "tinytime": "^0.2.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build:staging": "env-cmd .env.development npm-run-all -p build-css build-js",
    "build": "env-cmd .env.production npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/.",
    "fixlint": "eslint --fix src/.; exit 0",
    "stylelint": "stylelint \"src/**/*.scss\" --cache",
    "fix-stylelint": "stylelint --fix 'src/**/*.scss'; exit 0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "eslint-config-idiomatic": "^4.0.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.1.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "source-map-explorer": "^1.8.0",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.6.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "why-did-you-update": "^1.0.6"
  },
  "husky": {
    "hooks": {}
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
